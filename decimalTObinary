public class DecimalParaBinario {

	public void decimalparabin(int n) {

		String binario = "";
		while (n > 0) {

			int resto = n % 2;
			binario = resto + binario;
			n = n / 2;
		}

		System.out.print(binario);

	}

}
//Para q o código funcione, deve-se usar uma classe do tipo main e chamar este método
